{
  "name": "react-redux-feed",
  "description": "An higher order component to create and maintain feeds with react and redux from any data source",
  "main": "dist/index.js",
  "author": "Arijit Bhattacharya <hoodwink73@live.com>",
  "user": "hoodwink73",
  "license": "MIT",
  "version": "0.6.0",
  "dependencies": {
    "glamor": "^2.20.25",
    "glamorous": "^3.13.3",
    "is-url": "^1.2.2",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.4",
    "react-redux-feed": "^0.1.0",
    "react-virtualized": "^9.7.5",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.21.0",
    "@kadira/storybook-addon-knobs": "^1.7.1",
    "babel-cli": "^6.24.1",
    "babel-jest": "^20.0.3",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "date-fns": "^1.28.4",
    "enzyme": "^2.8.2",
    "express": "^4.15.3",
    "husky": "^0.13.3",
    "jest": "^20.0.4",
    "jest-cli": "^20.0.4",
    "jest-fetch-mock": "^1.1.1",
    "lint-staged": "^3.5.0",
    "np": "^2.15.0",
    "npm": "^4.6.1",
    "parse-link-header": "^1.0.0",
    "prettier": "^1.3.1",
    "react-addons-test-utils": "^15.5.1",
    "react-icons": "^2.2.4",
    "react-scripts": "1.0.7",
    "react-spinkit": "^2.1.2",
    "react-test-renderer": "^15.5.4",
    "twit": "^2.2.5",
    "typography": "^0.15.10"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "dist": "NODE_ENV=production babel lib -d dist --env production",
    "prepublish": "NODE_ENV=production babel lib -d dist --env production",
    "test": "jest --no-watchman --runInBand",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "dev-server": "node ./server/index.js",
    "release": "np --yolo --no-yarn --no-publish",
    "test:jest": "jest --no-watchman --runInBand",
    "watch:jest": "jest --no-watchman --watch",
    "prepush": "lint-staged"
  },
  "lint-staged": {
    "*.test.js": [
      "jest --no-watchman --runInBand"
    ],
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "setupFiles": [
      "<rootDir>/config/jest/setup.js"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|css|json)$)": "<rootDir>/config/jest/fileTransformer.js"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "__mocks__",
      "helpers"
    ],
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.js$"
    ],
    "collectCoverageFrom": [
      "src/**"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 2,
        "branches": 0.5,
        "functions": 1,
        "lines": 2
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/airwoot/react-redux-feed"
  },
  "keywords": [
    "react",
    "reactjs",
    "redux",
    "react-component",
    "list",
    "infinite",
    "virtualized",
    "feeds",
    "stream"
  ]
}
